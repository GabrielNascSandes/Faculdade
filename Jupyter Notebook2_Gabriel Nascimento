   ],
   "source": [
    "import numpy as np\n",
    "\n",
    "nonzero_payments = df[pay_features][df[pay_features] != 0]\n",
    "\n",
    "log_transformed = nonzero_payments.apply(lambda x: np.log10(x))\n",
    "\n",
    "plt.figure(figsize=(12, 8))\n",
    "\n",
    "for i, feature in enumerate(pay_features):\n",
    "    plt.subplot(2, 3, i+1)\n",
    "    log_transformed[feature].dropna().hist(bins=20)\n",
    "    plt.title(f'log10({feature})')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
   ]
